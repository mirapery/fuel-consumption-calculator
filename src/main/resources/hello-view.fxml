<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>


<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
      prefWidth="600.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="HelloController">

   <Button fx:id="btnEN" mnemonicParsing="false" text="EN" onAction="#onENClick"/>
   <Button fx:id="btnFR" mnemonicParsing="false" text="FR" onAction="#onFRClick"/>
   <Button fx:id="btnJP" mnemonicParsing="false" text="JP" onAction="#onJPClick"/>
   <Button fx:id="btnIR" mnemonicParsing="false" text="IR" onAction="#onIRClick"/>
   <Label fx:id="lblDistance" text="Label"/>
   <TextField fx:id="txtDistance"/>
   <Label fx:id="lblFuel" text="Label"/>
   <TextField fx:id="txtFuel"/>
   <Button fx:id="btnCalculate" mnemonicParsing="false" text="Button" onAction="#onCalculateClick"/>
   <Label fx:id="lblResult" text="Label"/>
</VBox>
